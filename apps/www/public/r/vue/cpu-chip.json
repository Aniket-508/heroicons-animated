\n  }\n};\n\nconst handleMouseEnter = () => {\n  if (!isControlled) startAnimation();\n};\n\nconst handleMouseLeave = () => {\n  if (!isControlled) stopAnimation();\n};\n\nconst setControlled = (value: boolean) => {\n  isControlled = value;\n};\n\ndefineExpose({ startAnimation, stopAnimation, setControlled });\n</script>\n",
